<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
  <mime-type type="application/x-flex-diskimage">
    <comment>FLEX disk image</comment>
    <glob pattern="*.dsk"/>
    <glob pattern="*.wta"/>
    <icon name="application-x-flex-diskimage"/>
  </mime-type>
  <mime-type type="application/x-flex-flxdiskimage">
    <comment>FLEX disk image</comment>
    <magic priority="100">
      <match value="0x339A5C48" type="big32" offset="0"/>
    </magic>
    <glob pattern="*.flx"/>
    <icon name="application-x-flex-diskimage"/>
  </mime-type>
</mime-info>

