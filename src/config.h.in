/* Following macros are generated by cmake. */
/* For this reason LINT macro checks have to be suppressed. */
/* NOLINTBEGIN(modernize-macro-to-enum, cppcoreguidelines-macro-usage) */

/* Define if this build uses CMAKE */
#cmakedefine USE_CMAKE 1

/* Define the CMAKE version when used for this build */
#cmakedefine CMAKE_VERSION "@CMAKE_VERSION@"

/* Define if you have the declaration of 'setenv' */
#cmakedefine HAVE_DECL_SETENV 1

/* Define if you have the declaration of 'unsetenv' */
#cmakedefine HAVE_DECL_UNSETENV 1

/* Define if you have the <fcntl.h> header file. */
#cmakedefine HAVE_FCNTL_H 1

/* Define if you have the <inttypes.h> header file. */
#cmakedefine HAVE_INTTYPES_H 1

/* Define if you have the <dirent.h> header file. */
#cmakedefine HAVE_DIRENT_H 1

/* Define if you have the <ndir.h> header file */
#cmakedefine HAVE_NDIR_H 1

/* Define if you have the <sys/dir.h> header file. */
#cmakedefine HAVE_SYS_DIR_H 1

/* Define if you have the <sys/ndir.h> header file. */
#cmakedefine HAVE_SYS_NDIR_H 1

/* Define if you have the <stdint.h> header file. */
#cmakedefine HAVE_STDINT_H 1

/* Define if you have the <sys/statvfs.h> header file. */
#cmakedefine HAVE_SYS_STATVFS_H 1

/* Define if you have the <sys/time.h> header file. */
#cmakedefine HAVE_SYS_TIME_H 1

/* Define if you have the <termios.h> header file. */
#cmakedefine HAVE_TERMIOS_H 1

/* Define if you have the <unistd.h> header file. */
#cmakedefine HAVE_UNISTD_H 1

/* Define if you have the <linux/joystick.h> header file. */
#cmakedefine LINUX_JOYSTICK_IS_PRESENT 1

/* Define byte size of some integer types */
#cmakedefine SIZEOF_SHORT @SIZEOF_SHORT@
#cmakedefine SIZEOF_INT @SIZEOF_INT@
#cmakedefine SIZEOF_LONG @SIZEOF_LONG@

/* Define WORDS_BIGENDIAN if your processor stores words with the most
  significant byte first (like Motorola and SPARC, unlike Intel). */
#cmakedefine WORDS_BIGENDIAN 1

/* Define BITFIELDS_LSB_FIRST if your processor stores bits in a bitfield
 * with the least significant bit first (like Intel, unlike Motorola and
 * SPARC). */
#cmakedefine BITFIELDS_LSB_FIRST 1

/* ncurses support */
/* define if library ncursesw is present */
#cmakedefine HAVE_NCURSESW 1
/* define if <curses.h> is present */
#cmakedefine HAVE_CURSES_H 1
/* define if <ncurses.h> is present */
#cmakedefine HAVE_NCURSES_H 1
/* define if <ncurses/ncurses.h> is present */
#cmakedefine HAVE_NCURSES_NCURSES_H 1
/* define if <ncurses/curses.h> is present */
#cmakedefine HAVE_NCURSES_CURSES_H 1
/* define if <ncursesw/ncurses.h> is present */
#cmakedefine HAVE_NCURSESW_NCURSES_H 1
/* define if <ncursesw/curses.h> is present */
#cmakedefine HAVE_NCURSESW_CURSES_H 1

#cmakedefine PACKAGE_NAME "@PACKAGE_NAME@"
#cmakedefine VERSION "@VERSION@"

/* NOLINTEND(modernize-macro-to-enum, cppcoreguidelines-macro-usage) */
